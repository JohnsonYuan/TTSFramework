<UserControl x:Uid="UserControl_1" x:Class="Microsoft.Tts.UI.Controls.Acoustic.WaveFormViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:customTrajectory="clr-namespace:Microsoft.Tts.UI.Controls.Trajectory"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="300">
    <Grid x:Uid="Grid_1">
        <customTrajectory:IntervalLinerGraph
            x:Uid="_linerGraph" x:Name="_linerGraph"
            IsEditable="False"
            IsHitTestVisible="False"
            CurveStrokeThickness="1"
            Upperbound="1e9"
            Lowerbound="-1e9" >
        </customTrajectory:IntervalLinerGraph>
        <Rectangle
            x:Uid="Rectangle_1" Stroke="Black"
            IsHitTestVisible="False" />
    </Grid>
</UserControl>
